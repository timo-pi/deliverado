<!-- route data -->
<div class="row">
  <div class="col-xs-12 col-sm-8 col-md-6 col-sm-offset-2 col-md-offset-3">
    <div class="form-banner "> <h4><%= @route.name %></h4> </div>
    <div class="route-card">
      <p>From: <%= @route.start_address %> </p>
      <p>To: <%= @route.end_address %> </p>
      <p>Radius: <%= @route.delivery_radius %> </p>
      <div class="btn-routes">
      <%= button_to "back", routes_path, method: 'get', class: "btn btn-primary form-btn" %>
    </div>
    </div>
  </div>
</div>

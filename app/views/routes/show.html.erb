<!-- route data -->
<div class="row">
  <div class="col-xs-10 col-sm-8 col-md-6 col-xs-offset-1 col-sm-offset-2 col-md-offset-3">
    <div class="route-card">
      <div class="form-banner "> <h4><%= @route.name %></h4> </div>
      <p>From: <%= @route.start_address %> </p>
      <p>To: <%= @route.end_address %> </p>
      <p>Radius: <%= @route.delivery_radius %> </p>
    </div>
    <div class="card-route-back">
      <%= link_to "back", routes_path %>
    </div>
  </div>
</div>
